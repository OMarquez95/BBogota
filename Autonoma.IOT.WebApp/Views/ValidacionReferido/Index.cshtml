}

<h2>@Resources.Titulos.lblMnuValidarRechazo</h2>
<hr>


<div class="container">

    <div class="row">
        <div class="col-lg-12 col-md-12">
            <div class="input-group">
                <input type="text" class="form-control" placeholder="Ingrese el Min" id="fatherMin" maxlength="10">
                <span class="input-group-btn">
                    <button class="btn btn-primary" type="button" id="consult">
                        Consultar
                    </button>
                </span>
            </div>
        </div>
    </div>

    <br />

    <div class="row">
        <div id="divContenedorGrilla" class="col-lg-12 col-md-12">

            @* @Html.Partial("~/Views/Referido/_grillaReferidos.cshtml", ViewData["test"])*@

            <div id="divResultados">
            </div>
        </div>
    </div>

    <br />
    <div class="alert alert-danger" role="alert" style="display:none;" id="error">

    </div>

    <div class="alert alert-warning" role="alert" style="display:none;" id="info">

    </div>

</div>

        <script type="text/javascript">
    var urlActivacion = {
        Pospago : "@this.Url.Action(Actions.ActivacionPospago, Controllers.ValidacionReferido)",
        Prepago : "@this.Url.Action(Actions.ActivacionPrepago, Controllers.ValidacionReferido)",
        Cadenas : "@this.Url.Action(Actions.ActivacionCadenas, Controllers.ValidacionReferido)",
        Migracion: "@this.Url.Action(Actions.ActivacionMigracion, Controllers.ValidacionReferido)",
        Index: "@this.Url.Action(Actions.ValidarReferido, Controllers.ValidacionReferido)",
        Causales: "@this.Url.Action(Actions.ConsultarCausalesTramite, Controllers.ValidacionReferido)",
        FiltrarRechazos: "@this.Url.Action(Actions.ConsultarRechazos, Controllers.ValidacionReferido)"
    };

